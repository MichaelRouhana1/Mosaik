# Copy this file to application-local.properties and add your Stripe keys.
# application-local.properties is gitignored and will not be committed.
#
# cp application-local.properties.example application-local.properties
# Then edit application-local.properties with your keys.
#
# stripe.secret.key: from https://dashboard.stripe.com/test/apikeys
# stripe.webhook.secret: from Stripe Dashboard > Developers > Webhooks (whsec_...)
#   Or use "stripe listen" CLI and copy the signing secret it prints.

stripe.secret.key=sk_test_your_key_here
stripe.webhook.secret=whsec_your_webhook_secret_here
